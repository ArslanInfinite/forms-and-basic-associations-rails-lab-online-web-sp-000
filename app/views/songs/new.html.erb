<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.text_field :note_ids, list: "notes_autocomplete" %>
  <datalist id="notes_autocomplete">
  <% Note.all.each do |note| %>
      <option value="<%= note.content %>">
    <% end %>
  </datalist>
  <textarea name="note[content]"></textarea>

  <%= f.submit %>
<% end %>